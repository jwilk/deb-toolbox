if [ $# = 0 ]
then
    printf 'Usage: %s CHANGES...\n' "${0#**/}" >&2
    exit 1
fi

exec dcmd rsync --chmod=0644 -P "$@" ssh.upload.debian.org:/srv/upload.debian.org/UploadQueue/

