if [ $# = 0 ]
then
    printf 'Usage: %s <changes>...\n' "$0" >&2
    exit 1
fi

dcmd rsync --chmod=0644 -P "$@" ssh.upload.debian.org:/srv/upload.debian.org/UploadQueue/

